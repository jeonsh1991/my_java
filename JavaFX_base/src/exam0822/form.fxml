<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="300.0" prefWidth="250.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <!-- TODO Add Nodes -->
  <bottom>
    <HBox alignment="CENTER" prefHeight="40.0" prefWidth="250.0">
      <children>
        <Button fx:id="btnSave" mnemonicParsing="false"  text="저장" />
        <Button fx:id="btnOut" mnemonicParsing="false" text="취소" />
      </children>
    </HBox>
  </bottom>
  <center>
    <GridPane alignment="CENTER" gridLinesVisible="false" maxWidth="200.0" prefHeight="100.0" prefWidth="300.0" BorderPane.alignment="CENTER">
      <children>
        <Label alignment="CENTER" text="이름" textAlignment="LEFT" wrapText="false" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="CENTER" />
        <TextField fx:id="textName" alignment="CENTER" maxWidth="100.0" prefHeight="-1.0" prefWidth="74.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="CENTER" />
        <Label alignment="CENTER" text="국어" textAlignment="CENTER" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
        <TextField id="textLang" fx:id="textKor" maxWidth="100.0"  prefWidth="200.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
        <Label alignment="CENTER" text="수학" textAlignment="CENTER" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
        <TextField fx:id="textMat" maxWidth="100.0"  prefWidth="200.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
        <Label alignment="CENTER" contentDisplay="LEFT" disable="false" text="영어" textAlignment="CENTER" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER" />
        <TextField fx:id="textEng" maxWidth="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES" />
        <RowConstraints fillHeight="true" minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
  </center>
  <padding>
    <Insets />
  </padding>
</BorderPane>
